#compdef nmap

_arguments -C \
  '-iL[Input from list of hosts/networks]:input file:_files' \
  '-iR[Choose random targets]:number of hosts' \
  '--exclude[Exclude hosts/networks]:excluded hosts' \
  '--excludefile[Exclude list from file]:exclude file:_files' \
  '-sL[Simply list targets to scan]' \
  '-sn[Disable port scan]' \
  '-Pn[Treat all hosts as online -- skip host discovery]' \
  '-PS[TCP SYN discovery]:port list' \
  '-PA[TCP ACK discovery]:port list' \
  '-PU[UDP discovery]:port list' \
  '-PY[SCTP discovery]:port list' \
  '-PE[ICMP echo discovery]' \
  '-PP[ICMP timestamp discovery]' \
  '-PM[ICMP netmask discovery]' \
  '-PO[IP Protocol Ping]:protocol list' \
  '-n[Never do DNS resolution]' \
  '-R[Always resolve DNS]' \
  '--dns-servers[Specify custom DNS servers]:DNS servers' \
  '--system-dns[Use OSs DNS resolver]' \
  '--traceroute[Trace hop path to each host]' \
  '-sS[TCP SYN scan]' \
  '-sT[TCP Connect scan]' \
  '-sA[TCP ACK scan]' \
  '-sW[TCP Window scan]' \
  '-sM[TCP Maimon scan]' \
  '-sU[UDP Scan]' \
  '-sN[TCP Null scan]' \
  '-sF[TCP FIN scan]' \
  '-sX[TCP Xmas scan]' \
  '--scanflags[Customize TCP scan flags]:flags' \
  '-sI[Idle scan]:zombie host[:probeport]' \
  '-sY[SCTP INIT scan]' \
  '-sZ[SCTP COOKIE-ECHO scan]' \
  '-sO[IP protocol scan]' \
  '-b[FTP bounce scan]:FTP relay host' \
  '-p[Only scan specified ports]:port ranges' \
  '--exclude-ports[Exclude the specified ports from scanning]:port ranges' \
  '-F[Fast mode - Scan fewer ports]' \
  '-r[Scan ports sequentially]' \
  '--top-ports[Scan number most common ports]:number of ports' \
  '--port-ratio[Scan ports more common than ratio]:ratio' \
  '-sV[Probe open ports to determine service/version info]' \
  '--version-intensity[Set version detection intensity level]:level' \
  '--version-light[Limit to most likely probes]' \
  '--version-all[Try every single probe]' \
  '--version-trace[Show detailed version scan activity]' \
  '-sC[Default script scan]' \
  '--script[Specify Lua scripts]:Lua scripts' \
  '--script-args[Provide arguments to scripts]:args' \
  '--script-args-file[Provide NSE script args in a file]:filename:_files' \
  '--script-trace[Show all data sent and received]' \
  '--script-updatedb[Update the script database]' \
  '--script-help[Show help about scripts]:Lua scripts' \
  '-O[Enable OS detection]' \
  '--osscan-limit[Limit OS detection to promising targets]' \
  '--osscan-guess[Guess OS more aggressively]' \
  '-T[Set timing template]:template number' \
  '--min-hostgroup[Parallel host scan group sizes]:size' \
  '--max-hostgroup[Parallel host scan group sizes]:size' \
  '--min-parallelism[Probe parallelization]:numprobes' \
  '--max-parallelism[Probe parallelization]:numprobes' \
  '--min-rtt-timeout[Probe round trip time]:time' \
  '--max-rtt-timeout[Probe round trip time]:time' \
  '--initial-rtt-timeout[Initial probe round trip time]:time' \
  '--max-retries[Caps number of port scan probe retransmissions]:tries' \
  '--host-timeout[Give up on target after this long]:time' \
  '--scan-delay[Adjust delay between probes]:time' \
  '--max-scan-delay[Adjust delay between probes]:time' \
  '--min-rate[Send packets no slower than]:number' \
  '--max-rate[Send packets no faster than]:number' \
  '-f[Fragment packets]' \
  '--mtu[Fragment packets with a given MTU]:val' \
  '-D[Cloak a scan with decoys]:decoys' \
  '-S[Spoof source address]:IP Address' \
  '-e[Use specified interface]:iface' \
  '-g[Use given port number]:portnum' \
  '--proxies[Relay connections through proxies]:url' \
  '--data[Append a custom payload to sent packets]:hex string' \
  '--data-string[Append a custom ASCII string to sent packets]:string' \
  '--data-length[Append random data to sent packets]:num' \
  '--ip-options[Send packets with specified IP options]:options' \
  '--ttl[Set IP time-to-live field]:val' \
  '--spoof-mac[Spoof your MAC address]:mac address/prefix/vendor name' \
  '--badsum[Send packets with a bogus checksum]' \
  '-oN[Output scan in normal format]:file:_files' \
  '-oX[Output scan in XML format]:file:_files' \
  '-oS[Output scan in s format]:file:_files' \
  '-oG[Output scan in Grepable format]:file:_files' \
  '-oA[Output in all formats]:basename' \
  '-v[Increase verbosity level]' \
  '-d[Increase debugging level]' \
  '--reason[Display the reason a port is in a particular state]' \
  '--open[Only show open ports]' \
  '--packet-trace[Show all packets sent and received]' \
  '--iflist[Print host interfaces and routes]' \
  '--append-output[Append to rather than clobber specified output files]' \
  '--resume[Resume an aborted scan]:filename:_files' \
  '--noninteractive[Disable runtime interactions via keyboard]' \
  '--stylesheet[Transform XML output to HTML]:path/URL' \
  '--webxml[Reference stylesheet from Nmap.Org for more portable XML]' \
  '--no-stylesheet[Prevent associating of XSL stylesheet with XML output]' \
  '-6[Enable IPv6 scanning]' \
  '-A[Enable OS detection, version detection, script scanning, and traceroute]' \
  '--datadir[Specify custom Nmap data file location]:dirname:_files' \
  '--send-eth[Send using raw ethernet frames]' \
  '--send-ip[Send using raw IP packets]' \
  '--privileged[Assume the user is fully privileged]' \
  '--unprivileged[Assume the user lacks raw socket privileges]' \
  '-V[Print version number]' \
  '-h[Print help summary page]'

